<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="pos_mx_pos_config_view_form" model="ir.ui.view">
        <field name="name">pos_mx.pos.config.form.viewt</field>
        <field name="model">pos.config</field>
        <field name="inherit_id" ref="point_of_sale.pos_config_view_form"/>
        <field name="arch" type="xml">
            <sheet position="inside">
                <h2>Pos mx</h2>
                <div class="col-xs-12 col-md-6 o_setting_box">
                    <div class="o_setting_left_pane">
                        <field name="envio_salida_vencimiento_id"/>
                    </div>
                    <div class="o_setting_right_pane">
                        <label for="envio_salida_vencimiento_id" string="Envio de salida por vencimiento"/>
                    </div>
                </div>
            </sheet>
        </field>
    </record>




</odoo>